{
	"parser": "babel-eslint",
	"extends": "airbnb",
	"plugins": [
		"react",
		"import"
	],
	"rules": {
		"indent": ["error", "tab"],
		"no-tabs": 0,
		"react/jsx-indent": ["error", "tab"],
		"react/jsx-filename-extension": [1, {
			"extensions": [".js", ".jsx"]
		}],
		"jsx-a11y/anchor-is-valid": 0,
		"react/prefer-stateless-function": 0,
		"import/first": 0
	},
	"env": {
		"browser": true
	}
}