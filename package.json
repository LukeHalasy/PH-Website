{
	"name": "backend",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"format": "prettier --write \"{src,__}/**/*.ts\" --find-config-path",
		"build": "tsc && cp -r backend/emails dist/emails",
		"watch": "tsc -w",
		"start": "nodemon dist/index.js",
		"dev":
			"nodemon -i \"**/*.test.ts\" -i \"**/*.spec.ts\" -i \"node_modules\" -w backend -e ts -x \"npm run server\"",
		"deploy": "gcloud app deploy",
		"server": "ts-node --inspect=5858 backend/index.ts"
	},
	"dependencies": {
		"@google-cloud/storage": "^1.7.0",
		"bcrypt": "^2.0.1",
		"body-parser": "^1.18.2",
		"cookie-parser": "~1.4.3",
		"cors": "^2.8.4",
		"crypto-js": "^3.1.9-1",
		"debug": "~2.6.9",
		"dotenv": "^5.0.1",
		"express": "~4.16.0",
		"express-paginate": "^1.0.0",
		"express-session": "^1.15.6",
		"handlebars": "^4.0.11",
		"http-errors": "^1.6.3",
		"jsonwebtoken": "^8.2.1",
		"mongodb": "^3.1.0-beta4",
		"mongoose": "^5.1.2",
		"morgan": "~1.9.0",
		"multer": "^1.3.1",
		"nodemailer": "^4.6.7",
		"passport": "^0.4.0",
		"passport-jwt": "^4.0.0",
		"validator": "^10.2.0",
		"nodemon": "^1.17.3",
		"ts-node": "^7.0.0",
		"typescript": "^2.8.3"
	},
	"devDependencies": {
		"@types/bcrypt": "^2.0.0",
		"@types/body-parser": "^1.17.0",
		"@types/cookie-parser": "^1.4.1",
		"@types/cors": "^2.8.4",
		"@types/crypto-js": "^3.1.39",
		"@types/express": "^4.11.1",
		"@types/express-session": "^1.15.8",
		"@types/google-cloud__storage": "^1.7.0",
		"@types/gridfs-stream": "^0.5.30",
		"@types/handlebars": "^4.0.39",
		"@types/http-errors": "^1.6.1",
		"@types/jsonwebtoken": "^7.2.7",
		"@types/mongodb": "^3.0.18",
		"@types/mongoose": "^5.0.12",
		"@types/morgan": "^1.7.35",
		"@types/multer": "^1.3.7",
		"@types/node": "^10.0.3",
		"@types/nodemailer": "^4.6.2",
		"@types/passport": "^0.4.5",
		"@types/passport-jwt": "^3.0.1",
		"@types/validator": "^9.4.1",
		"concurrently": "^3.5.1",
		"eslint": "^5.3.0",
		"eslint-plugin-typescript": "^0.12.0",
		"prettier": "^1.12.1",
		"prettier-eslint": "^8.8.2",
		"typescript-eslint-parser": "^18.0.0"
	},
	"engines": {
		"node": "10.x",
		"npm": "6.x",
		"yarn": "1.9.x"
	}
}
